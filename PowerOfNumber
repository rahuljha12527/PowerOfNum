import java.util.Scanner;
public class Main {
	
	public static void main(String[] args) {
		// Write your code here

        Scanner sc=new Scanner(System.in);
        
        int t=sc.nextInt();
        int y=sc.nextInt();
        int c=1;
        for(int i=0;i<y;i++){
            
             c=t*c;
        }
        System.out.println(c);
        
	}
}
